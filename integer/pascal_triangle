class Solution:
    def generate(self, numRows: int) -> List[List[int]]:
        triangle = [[1]]
        
        for i in range(1, numRows):
            prev_row = triangle[-1]
            curr_row = [1]
            for j in range(1, i):
                curr_row.append(prev_row[j-1] + prev_row[j])
            
            # Every row ends with 1
            curr_row.append(1)
            triangle.append(curr_row)
            
        return triangle
