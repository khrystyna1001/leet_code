class Solution:
    def generateParenthesis(self, n: int) -> List[str]:
        res = []
        
        def backtrack(open_count, close_count, path):
            # Base Case: We've used all n open and n close brackets
            if len(path) == 2 * n:
                res.append("".join(path))
                return
            # Rule 1: We can always add an '(' if we have some left
            if open_count < n:
                path.append("(")
                backtrack(open_count + 1, close_count, path)
                path.pop() # Backtrack
            
            # Rule 2: We can only add a ')' if it closes an existing '('
            if close_count < open_count:
                path.append(")")
                backtrack(open_count, close_count + 1, path)
                path.pop() # Backtrack
        backtrack(0, 0, [])
        return res
