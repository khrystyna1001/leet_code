class Solution:
    def climbStairs(self, n: int) -> int:
        
        def climbMemo(n, memo={}):
            if n in memo:
                return memo[n]
            if n == 0 or n == 1:
                return 1
            memo[n] = climbMemo(n - 1, memo) + climbMemo(n - 2, memo)
            return memo[n]
        
        return climbMemo(n)
